{
  "name": "uzman-defter-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref ruztowpsuplphhoukcer",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "supabase:types": "npx supabase gen types typescript --project-id ruztowpsuplphhoukcer --schema public > database/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "iconify-icon": "^3.0.0",
    "lucide-vue-next": "^0.541.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "supabase": "^2.34.3",
    "typescript": "~5.8.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
